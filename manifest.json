{
    "manifest_version": 3,
    "name": "FraudDef",
    "description": "An extension which checks URLs' safety",
    "version": "1.0",
    "icons": {
        "48": "icon.png",
        "128": "icon.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "permissions": ["activeTab", "tabs"]
}
